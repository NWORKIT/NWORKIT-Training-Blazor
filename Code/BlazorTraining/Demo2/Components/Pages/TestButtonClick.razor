@page "/test-click"
<h3>Click test</h3>

<button type="button" @onclick="TestClick">Click me</button>

<p>@message</p>


<p>
    In <b>app.razor</b> it was added <b>rendermode="InteractiveServer"</b>.

</p>


@code {
    private string message = "";

    private void TestClick()
    {
        message = "Button clicked!";
    }
}
